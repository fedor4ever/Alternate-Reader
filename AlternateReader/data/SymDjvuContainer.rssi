			type = EAknCtMessageQuery;
			id = EAknMessageQueryContentId;
			control = AVKON_MESSAGE_QUERY
				{
				};
			}
		};
	}

RESOURCE DIALOG r_find_data_query
{
	flags = EGeneralQueryFlags;
	buttons = R_AVKON_SOFTKEYS_SHOW_CANCEL;
	items =
		{
		DLG_LINE
			{
				type = EAknCtQuery;
				id = EGeneralQuery;
				control = AVKON_DATA_QUERY
					{
						layout = EDataLayout;
						label = ""; // dialof title
						control = EDWIN
							{
								width = 5;
								lines = 1;
								maxlength = 15;
							};
					};
			}
		};
}

