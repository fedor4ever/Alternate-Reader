		MENU_ITEM
			{
				txt = STR_SymDjvuContainerViewLibrary;
				command = EOpenLibrary;
			},
		MENU_ITEM
			{
				txt = STR_SymDjvuContainerViewTOC;
				command = EOpenTOC;
			},
		MENU_ITEM
			{
				txt = STR_SymDjvuContainerViewBookmarks;
				command = EOpenBookmarks;
			},
		MENU_ITEM
			{
				txt = STR_SymDjvuContainerViewFindText;
				command = EFindText;
			}

		};
}

